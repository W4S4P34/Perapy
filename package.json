{
  "name": "ui-project-perapy",
  "version": "1.0.0",
  "private": true,
  "description": "Pet-oriented frontend only website",
  "author": "Perapy Team @ HCMUS",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:app": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:css": "postcss build src/styles/tailwind.css -o dist/styles.css",
    "watch": "watch 'npm run build:css' src/styles"
  },
  "main": "src/main.js",
  "dependencies": {
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "@vue/cli": "^4.5.13",
    "autoprefixer": "^9",
    "core-js": "^3.6.5",
    "postcss": "^7",
    "postcss-cli": "^7",
    "postcss-import": "^12",
    "postcss-loader": "^6.1.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "vue": "^3.0.0",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "vue-cli-plugin-tailwind": "~2.0.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "bugs": {
    "url": "https://github.com/alecbuivhp/ui-project-perapy/issues"
  },
  "homepage": "https://github.com/alecbuivhp/ui-project-perapy#readme",
  "keywords": [
    "pet",
    "web",
    "vuejs",
    "tailwindcss"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alecbuivhp/ui-project-perapy.git"
  }
}
